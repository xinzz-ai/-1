int my_strcmp(const char *a, const char *b)
{
    const char *m = a;
    const char *n = b;

    while (*m != '\0' && *n != '\0') {
        if (*m != *n) {
            return (int)(*m - *n);
        }
        m++;
        n++;
    }

    return (int)(*m - *n);
}


